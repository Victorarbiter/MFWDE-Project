{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-console": "off",
    "no-extra-semi": 0,
    "no-undef": "off",
    "consistent-return": "off",
    "no-restricted-globals":"off",
    "object-curly-newline":"off",
    "no-prototype-builtins": "off",
    "import/no-cycle": "off"
  }
}